<?xml version="1.0" encoding="utf-8"?>
<paths xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 外部存储的图片目录 -->
    <external-files-path
        name="pictures"
        path="Pictures/" />

    <!-- 外部存储的视频目录 -->
    <external-files-path
        name="movies"
